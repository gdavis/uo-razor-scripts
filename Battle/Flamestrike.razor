hotkey 'Clear Target Queue'

if targetexists 
    hotkey 'Cancel Current Target'
endif

cooldown 'Spell Precast' 0
clearsysmsg 
cast 'Flamestrike'

while not targetexists 
endwhile

cooldown 'Spell Precast' 35000

while targetexists 
endwhile

if cooldown 'Spell Precast' <= 0
    overhead 'Cast timed out!' 38
else
    cooldown 'Spell Precast' 0
endif