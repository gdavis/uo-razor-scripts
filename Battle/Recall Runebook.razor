hotkey 'Recall'
waitfortarget

if find recallTarget backpack
    target recallTarget
elseif findtype 'runebook' backpack
    targettype 'runebook' backpack
elseif findtype 'runetome' backpack
    targettype 'runetome' backpack
elseif findtype 'recall rune' backpack
    targettype 'recall rune' backpack
else
    overhead 'Not sure what to recall to!' 38
endif