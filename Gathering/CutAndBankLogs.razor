if findtype 'log%s' backpack
    overhead 'Cutting logs' 53
    dclicktype 'scissors' backpack
    waitfortarget 3000
    targettype 'log%s' backpack
    wait 750
    
    overhead 'Moving to bank' 903
    organizer 11
    wait 2000
    loop
else 
    # add plant stuff to shelf
    if findtype 'garden shelf' ground any any any 2 as shelf
        menu shelf 0
        wft 3000
        target self
    endif

    overhead 'Logs cut and banked.'
endif