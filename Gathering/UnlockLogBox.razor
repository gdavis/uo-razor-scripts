# open the supplies bag under trapped pouches
dclick 'logBagPouch'
wait 1000

# pickup key from bank
lift 'logBoxKey' 1
drop 0x470FA367 88 101 0

# use on lockbox
clearsysmsg 
dclick 'logBoxKey'
waitfortarget
target 'logBox'
wait 500

# try to open it
clearsysmsg 
dclick 'logBox'
wait 500

# if its locked, repeat
if insysmsg 'It appears to be locked'
    gumpclose
    loop
endif

# move key back into bank
lift 'logBoxKey' 1
drop 0x470FA36D 86 103 0